<?xml version="1.0" encoding="UTF-8"?>
<!--
Configuration file used to handle global properties for Application.
-->
<config version="2.0" application="voipcarrier">
    <properties>
        <!-- Define the location for the cpnfiguration file -->
        <property name="voipcarrier.properties" type="string">/cf/properties/voipcarrier.properties</property>
        <!-- Default the location for the appconn configuration file  -->
        <property name="app-client.properties" type="string">/cf/properties/voipcarrier.gw1.properties</property>
        <!-- Indicates if the application is running in test mode. It must to be disable for production -->
        <property name="enable-test-mode" type="boolean">true</property>
    </properties>
    <exclude-sip-headers>
        <sip-header>100rel</sip-header>
    </exclude-sip-headers>
</config>
